@model List<LMS.Models.QueueList>
@{
    ViewBag.Title = "Queue List";
}


<section class="content-header">
    <h1>
        Queue List
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Data</a></li>
        <li class="active">Queue</li>
    </ol>
</section>
<section class="content">
    @{ string UserFullName = "";
     string UserType = "0";
     string UserTypeName = "";
     string AgentID = "";
     if (HttpContext.Current.Session["LogedUserFullname"] != null)
     {
         UserFullName = HttpContext.Current.Session["LogedUserFullname"].ToString();
         AgentID = HttpContext.Current.Session["LogedAgentID"].ToString();
         UserType = HttpContext.Current.Session["LogedUserType"].ToString();

         if (UserType == "1")
         {
             UserTypeName = "Internal";
         }
         else if (UserType == "2")
         {
             UserTypeName = "Sub Agent";
         }
     }

    }
    <div class="row">
        <div class="col-md-12">

            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Queue List</h3>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-minus"></i>
                        </button>
                        <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                    </div>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <div class="table-responsive">
                        <table class="table no-margin">
                            <thead>
                                <tr>
                                  
                                    <th>License Plate</th>
                                    <th>Model</th>
                                    <th>Driver Name</th>
                                    <th>Last Booking</th>
                                    <th>Last Job (Date)</th>
                                    <th>Last Job (Time)</th>
                                    <th>Status</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                  
                                    <tr>
                                        <td><a href="/LMS/Queue">@item.VehicleRegis</a></td>
                                        <td>@item.Model</td>
                                        <td>@item.DTitle &nbsp; @item.DFirstName &nbsp; @item.DLastName</td>
                                        <td>@item.LastBooking</td>
                                        <td>@item.LastJobDate</td>
                                        <td>@item.LastJobTime</td>
                                        <td>@item.TRemark</td>
                                    </tr>


                                }


                            </tbody>
                        </table>
                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.box-body -->
                @*<div class="box-footer clearfix">
                    <a href="/LMS/NewDriver" class="btn btn-sm btn-info btn-flat pull-right">New Car</a>
                    <a href="javascript:void(0)" class="btn btn-sm btn-default btn-flat pull-left">View All</a>
                </div>*@
                <!-- /.box-footer -->
            </div>

        </div>

    </div>

</section>
