            }
            else {

                counter = document.getElementById("limit").value;
                //     var AddDetail = document.getElementById("AddDetail").value;

                var Detail = 'Detail' + counter;
                var Num = 'Num' + counter;
                var oDetail = 'oDetail' + counter;
                var oRemove = counter;


                var newdiv = document.createElement('div');
                newdiv.innerHTML = "<div class='form-group' id=" + oDetail + " name=" + oDetail + ">";
