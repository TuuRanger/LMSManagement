        document.getElementById('Service2').style.display = 'none';
        document.getElementById('Service3').style.display = 'none';


        function showhidediv(rad) {
            var rads = document.getElementsByName(rad.name);
            document.getElementById('Service1').style.display = (rads[0].checked) ? 'block' : 'none';
            document.getElementById('Service2').style.display = (rads[1].checked) ? 'block' : 'none';
            document.getElementById('Service3').style.display = (rads[2].checked) ? 'block' : 'none';

            document.getElementById('ServiceType').value = (rads[0].checked) ? '1' : '3';
            document.getElementById('ServiceType').value = (rads[2].checked) ? '3' : '1';

        }

        function showhidedivC(rad) {
            var rads = document.getElementsByName(rad.name);
            document.getElementById('SubAgent').style.display = (rads[1].checked) ? 'block' : 'none';
            document.getElementById('CustomerType').value = (rads[1].checked) ? '2' : '1';

        }
    </script>
